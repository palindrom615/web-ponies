{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": false,
        "no-console": false,
        "trailing-comma": false,
        "member-access": false,
        "quotemark": [true, "single"],
        "interface-over-type-literal": false,
        "only-arrow-functions": false
    },
    "rulesDirectory": []
}
